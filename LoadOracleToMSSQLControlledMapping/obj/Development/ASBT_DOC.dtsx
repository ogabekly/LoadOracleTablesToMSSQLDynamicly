                            //        sqlBulkCopy.ColumnMappings.Add(sourceColumn, targetColumn);
                            //    }
                            //}

                            sqlBulkCopy.WriteToServer(dataTable);
                        }
                        sqlConnection.Close();
                    }
                }
            }

            MessageBox.Show("Data transfer complete.");

            Dts.TaskResult = (int)ScriptResults.Success;
        }

